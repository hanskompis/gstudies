

SELECT DISTINCT opiskelija.nimi, opinkohd.TUNNISTE, opinkohd.NIMI, opinstat.SELITE, opinto.KIRJPVM from opinkohd, opinto, opiskelija,opinstat WHERE opinto.OPINSTAT = opinstat.KOODI AND opinto.HLO = opiskelija.HLO AND opinto.OPINKOHD = opinkohd.OPINKOHD AND opiskelija.HLO = 51279398 AND opinkohd.TUNNISTE IN ('58131','581325','582103','582104', '581305', '581328', '58160','582203','582102','57049','581324','581326','582101','581329') AND opinstat.KOODI IN (4,7,9) GROUP BY opiskelija.NIMI ORDER BY opinto.SUORPVM ASC

SELECT opinkohd.TUNNISTE, opinkohd.NIMI from opinkohd, opinto, opiskelija WHERE opinto.HLO = opiskelija.HLO AND opinto.OPINKOHD = opinkohd.OPINKOHD AND opiskelija.HLO = 51279398

opiskelijat:**********************
SELECT DISTINCT opiskelija.HLO FROM opiskelija, opinto, opinkohd, opinstat WHERE opinto.OPINSTAT = opinstat.KOODI AND opinto.HLO = opiskelija.HLO AND opinto.OPINKOHD = opinkohd.OPINKOHD AND opinkohd.TUNNISTE IN ('58131','581325','582103','582104', '581305', '581328', '58160','582203','582102','57049','581324','581326','582101','581329') AND opinstat.KOODI IN (4,7,9)

***opiskelijat mukaan listauksiin************
SELECT DISTINCT opiskelija.HLO, opinkohd.TUNNISTE, opinkohd.NIMI, opinstat.SELITE, opinto.SUORPVM from opinkohd, opinto, opiskelija,opinstat WHERE opinto.OPINSTAT = opinstat.KOODI AND opinto.HLO = opiskelija.HLO AND opinto.OPINKOHD = opinkohd.OPINKOHD AND opiskelija.HLO = 51279398 AND opinkohd.TUNNISTE IN ('58131','581325','582103','582104', '581305', '581328', '58160','582203','582102','57049','581324','581326','582101','581329') AND opinstat.KOODI IN (4,7,9) GROUP BY opiskelija.HLO ORDER BY opinto.SUORPVM ASC
